{
    "name": "@nuxt-alt/auth",
    "version": "3.0.0-beta.0",
    "description": "An alternative module to @nuxtjs/auth",
    "homepage": "https://github.com/nuxt-alt/auth",
    "author": "Denoder",
    "keywords": [
        "nuxt",
        "nuxtjs",
        "nuxt-module",
        "nuxt-plugin",
        "nuxt-module-alternatives",
        "@nuxtjs/auth"
    ],
    "license": "MIT",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "module": "./dist/module.mjs",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "unbuild --stub && nuxi prepare playground",
        "prepack": "unbuild"
    },
    "dependencies": {
        "@nuxt-alt/http": "latest",
        "@nuxt/kit": "^3.8.1",
        "cookie-es": "^1.0.0",
        "defu": "^6.1.3",
        "jwt-decode": "^4.0.0",
        "ohash": "^1.1.3",
        "pathe": "^1.1.1",
        "pinia": "^2.1.7",
        "requrl": "^3.0.2"
    },
    "devDependencies": {
        "@pinia/nuxt": "^0.5.1",
        "@types/node": "^18",
        "nuxt": "^3.8.1",
        "typescript": "^5.2.2",
        "unbuild": "^2.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nuxt-alt/auth.git",
        "directory": "@nuxt-alt/auth"
    },
    "bugs": {
        "url": "https://github.com/nuxt-alt/auth/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "yarn@3.3.1"
}
